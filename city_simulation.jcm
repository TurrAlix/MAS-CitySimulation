mas city_simulation {

    agent car : car.asl  {
        focus: city.view
    }
    
    agent car2 : car.asl  {
        focus: city.view2
    }

    agent pedestrian : pedestrian.asl  {
        focus: city.view3
    }

    workspace city {
        // first parameter is the id of the city 
        // second parameter is the id of the agent in the scenario
        artifact view: city.City(3, 0)
        artifact view2: city.City(3, 1)
        artifact view3: city.City(3, 2)
    }
}

